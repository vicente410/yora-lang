# This program asks for a number in loop and prints if the number
# is prime or not. It stops once it recieves a 0.

pr is_prime(num: Int) -> Bool:
    var div = 2
    while div * div <= num:
        if num % div == 0:
            break
        div += 1
    
    return div * div > num

var input = 1
while input > 0:
	input = string_to_int(input())
	print(is_prime(input))
